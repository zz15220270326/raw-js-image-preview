{
  "name": "@jackzhang0715/raw-js-image-preview",
  "version": "1.0.0.1",
  "description": "An ImagePreview Component implied by raw js (actually typescript) (no VueJS or balala)",
  "main": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "dev:express": "nodemon ./app.js",
    "build:dev": "cross-env NODE_ENV=development ./node_modules/.bin/rollup -c ./rollup.config.js -w",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/rollup -c ./rollup.config.js",
    "build:css": "./node_modules/.bin/sass ./lib/styles/index.scss ./dist/styles/index.css -w --style compressed"
  },
  "keywords": [
    "ImagePreview",
    "JavaScript",
    "oop"
  ],
  "author": "jackzhang0715",
  "license": "ISC",
  "bugs": {},
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.0",
    "cross-env": "^7.0.3",
    "rollup": "^3.26.2",
    "rollup-plugin-extensions": "^0.1.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "tslib": "^2.6.0",
    "typescript": "4.x",
    "vite": "^4.4.1"
  },
  "dependencies": {
    "@jackzhang0715/raw-js-image-preview": "1.0.0",
    "animate.css": "^4.1.1",
    "ejs": "^3.1.9"
